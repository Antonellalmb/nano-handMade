<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head') %>
<body>
  <!-- Preload the Whole Page -->
  <div id="preloader">      
    <div id="loading-animation">&nbsp;</div>
  </div>
        
  <!-- Navbar -->
  <%- include('../partials/header') %>
  <!-- End Navbar -->
  <main class="mainInput">
    <div class="tableInput">
  
        <h4>Editar Categoría</h4>
        <div class="formCategorias">
          <form action="/user/guardarEdicionCategoria" method="post">
            <div>
              
              <input type="hidden" name="categoriaIdEditar" value="<%= categoria.id %>">
              <label class="labelNovaCategoria" for="nuevaCategoria"> Editar</label>
              <input type="text" name="nuevaCategoria" value="<%= categoria.roles %>" required>
            </div>
            <div class="botonsInput">
              <button class="botEnterTablas tablasBoton" type="submit">Salvar alterações</button>
            </div>
          </form>
        </div>
        
        <!-- para eliminar la categoría -->
        <button class="botEnterTablas tablasBoton" type="submit"> <a href="/user/eliminarCategoria/<%= categoria.id %>">Eliminar Categoría</a> </button>
        
        <!--  para volver a la lista de categorías -->
        <button class="botEnterTablas tablasBoton" type="submit"> <a href="/user/categorias">Retornar à tabela de categorias</a> </button>
 <!-- Footer
    ================================================== -->
    <%- include('../partials/footer') %> 
    <!-- End footer -->
