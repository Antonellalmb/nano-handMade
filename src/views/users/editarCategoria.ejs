<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head') %>
<body>
  <!-- Preload the Whole Page -->
  <div id="preloader">      
    <div id="loading-animation">&nbsp;</div>
  </div>
        
  <!-- Navbar -->
  <%- include('../partials/header') %>
  <!-- End Navbar -->
  <main class="mainInput">
    <div class="tableInput">
  
        <h4>Editar Categoría</h4>
        <form action="/user/guardarEdicionCategoria" method="post">
          <div>
            <input type="hidden" name="categoriaIdEditar" value="<%= categoria.id %>">
            <label for="nuevaCategoria">Nueva Categoría</label>
            <input type="text" name="nuevaCategoria" value="<%= categoria.roles %>" required>
          </div>
          <div class="botonsInput">
            <button class="botEnterTablas tablasBoton" type="submit">Guardar Cambios</button>
          </div>
        </form>
        
        <!-- para eliminar la categoría -->
        <a href="/user/eliminarCategoria/<%= categoria.id %>">Eliminar Categoría</a>
        
        <!--  para volver a la lista de categorías -->
        <a href="/user/categorias">Retornar à tabela de categorias</a>
 <!-- Footer
    ================================================== -->
    <%- include('../partials/footer') %> 
    <!-- End footer -->
     <!-- JS libraries and scripts -->
  <script src="/js/jquery-1.11.3.min.js"></script>
  <script src="/js/bootstrap.min.js"></script>
